  /* Base styles for all arrows */
  [data-arrow-up],
  [data-arrow-down],
  [data-arrow-up-right],
  [data-arrow-up-right-cta] {
    transition: transform 0.4s ease;
    will-change: transform;
    backface-visibility: hidden;
  }
  /* Rotated arrow up */
  [data-arrow-up] {
    transform: rotate(180deg);
  }

  /* === Hover-triggered animations === */
  [data-arrow-parent]:hover [data-arrow-up] {
    animation: arrow-up 0.75s ease forwards;
  }
  [data-arrow-parent]:hover [data-arrow-up-right] {
    animation: arrow-up-right 0.75s ease forwards;
  }
  [data-arrow-parent]:hover [data-arrow-up-right-cta] {
    transform: translate(10px, -10px);
    transition: transform 0.4s ease;
  }
  /* Reset CTA arrow when hover ends with an improved transition */
  [data-arrow-up-right-cta] {
    transition: transform 0.4s ease;
  }

  /* Apply same speed as arrow-up, but delay each loop for 2s */
  [data-arrow-down] {
    animation: arrow-down 2.75s ease infinite;
  }
  /* === Keyframes === */
  @keyframes arrow-up {
    0%   { transform: rotate(180deg) translateY(0); opacity: 1; }
    25%  { transform: rotate(180deg) translateY(100%); opacity: 0.8; }
    26%  { transform: rotate(180deg) translateY(-100%); opacity: 0; }
    40%  { opacity: 0.8; }
    100% { transform: rotate(180deg) translateY(0); opacity: 1; }
  }
  @keyframes arrow-up-right {
    0%   { transform: translate(0, 0); opacity: 1; }
    25%  { transform: translate(120%, -120%); opacity: 0.8; }
    26%  { transform: translate(-80%, 80%); opacity: 0; }
    40%  { opacity: 0.8; }
    100% { transform: translate(0, 0); opacity: 1; }
  }

  @keyframes arrow-down {
    0%   { transform: translateY(0); opacity: 1; }
    6.8%  { transform: translateY(100%); opacity: 0.8; }
    7%  { transform: translateY(-100%); opacity: 0; }
    10.9% { opacity: 0.8; }
    25% { transform: translateY(0); opacity: 1; }
    100% { transform: translateY(0); opacity: 1; } /* idle time until next loop */
  }

  /* Base styles for tab date elements */
  [data-tab-date] {
    transition: transform 0.4s ease;
    will-change: transform;
    backface-visibility: hidden;
  }
  /* Hover-triggered animation */
  [data-tab]:hover [data-tab-date] {
    animation: tab-date-up 0.75s ease forwards;
  }
  /* Keyframes for the tab date up animation */
  @keyframes tab-date-up {
    0%   { transform: translateY(0); opacity: 1; }
    25%  { transform: translateY(-100%); opacity: 0.8; }
    26%  { transform: translateY(100%); opacity: 0; }
    40%  { opacity: 0.8; }
    100% { transform: translateY(0); opacity: 1; }
  }

  body.wfc25-theme-light .wfc25-random-logo {
    mix-blend-mode: difference;
  }
	
  /* Controls accordion colors when expanded and hover */
  .accordion-trigger:hover .accordion-icon_right {
    background-color: var(--_webflow-conf-2025---link);
    color: var(--_webflow-conf-2025---background-primary);
  }
  
  .accordion-trigger[aria-expanded="true"] .accordion-icon_right {
    background-color: var(--_webflow-conf-2025---link);
    color: var(--_webflow-conf-2025---background-primary);
  }

  /* Theme switcher icon animation */
  .theme-toggle_icon {
    transition: transform 0.4s ease, opacity 0.4s ease;
  }

  /* Animate out before hiding */
  .theme-toggle_icon.icon-out {
    transform: scale(0.8) rotate(-45deg);
    opacity: 0;
  }

  /* Animate in when showing */
  .theme-toggle_icon.icon-in {
    animation: iconIn 0.4s forwards ease;
  }

  @keyframes iconIn {
    0% {
      transform: scale(0.8) rotate(-45deg);
      opacity: 0;
    }
    100% {
      transform: scale(1) rotate(0deg);
      opacity: 1;
    }
